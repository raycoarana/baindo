<manifest xmlns:android="http://schemas.android.com/apk/res/android"
		  package="com.raycoarana.baindo.sample">

	<application android:allowBackup="true"
				 android:label="@string/app_name"
                 android:name=".app.SampleApplication"
				 android:icon="@drawable/ic_launcher"
				 android:theme="@style/AppTheme">

		<activity android:name=".main.view.MainActivity">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
        <activity android:name=".click.view.ClickActivity"/>
        <activity android:name=".text.view.TextActivity"/>

	</application>

</manifest>
